    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.3.2/html2canvas.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.4.0/jspdf.umd.min.js"></script>
    <script>
        async function downloadPDF2() {
            try {
                // Ocultar elementos no deseados antes de la captura
                document.querySelectorAll('nav, aside, button').forEach(el => el.style.display = 'none');

                const content = document.getElementById('content1'); // El contenedor que deseas convertir a PDF
                const canvas = await html2canvas(content, {
                    scale: 1,
                    useCORS: true // Para manejar el contenido de origen cruzado (si aplica)
                });

                // Volver a mostrar elementos ocultos
                document.querySelectorAll('nav, aside, button').forEach(el => el.style.display = '');

                const imgData = canvas.toDataURL('image/png');
                const { jsPDF } = window.jspdf;
                const pdf = new jsPDF({
                    orientation: 'portrait',
                    unit: 'pt',
                    format: 'a4'
                });

                const imgProps = pdf.getImageProperties(imgData);
                const pdfWidth = pdf.internal.pageSize.getWidth();
                const pdfHeight = pdf.internal.pageSize.getHeight();
                let imgHeight = (imgProps.height * pdfWidth) / imgProps.width;
                let heightLeft = imgHeight;

                let position = 0;

                pdf.addImage(imgData, 'PNG', 0, position, pdfWidth, imgHeight);
                heightLeft -= pdfHeight;

                while (heightLeft >= 0) {
                    position = heightLeft - imgHeight;
                    pdf.addPage();
                    pdf.addImage(imgData, 'PNG', 0, position, pdfWidth, imgHeight);
                    heightLeft -= pdfHeight;
                }

                pdf.save('details.pdf');
            } catch (error) {
                console.error('Error al generar PDF', error);
            }
        }
    </script>

